{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Debug",
            "runtimeArgs": ["--require", "ts-node/register"],
            "program": "${workspaceFolder}/src/index-local.ts",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "outFiles": [
                "${workspaceFolder}/**/*.js",
            ],
            "cwd": "${workspaceFolder}"
        }
    ]
}